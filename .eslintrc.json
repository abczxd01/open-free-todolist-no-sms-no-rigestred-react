{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "settings": {
    "import/extensions": [".js", ".jsx"]
  },
  "rules": {
    "arrow-parens":["error","as-needed"],
    "import/no-default-export": 2,
    "import/prefer-default-export":0
  }
}
